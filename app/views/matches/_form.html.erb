  </div>
  <div class="field">
    <%= f.label :available1_id %><br>
    <%= f.number_field :available1_id %>
  </div>
  <div class="field">
    <%= f.label :available2_id %><br>
    <%= f.number_field :available2_id %>
  </div>
  <div class="field">
    <%= f.label :user1_response? %><br>
    <%= f.check_box :user1_response? %>
  </div>
  <div class="field">
    <%= f.label :user2_response? %><br>
    <%= f.check_box :user2_response? %>
  </div>
  <div class="field">
    <%= f.label :user1_commitment %><br>
    <%= f.number_field :user1_commitment %>
  </div>
  <div class="field">
    <%= f.label :user2_commitment %><br>
